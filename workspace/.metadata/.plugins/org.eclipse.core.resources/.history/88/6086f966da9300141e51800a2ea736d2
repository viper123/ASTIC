			try{
			DriverManager.getConnection(
					"jdbc:oracle:thin:@localhost:1521:XE", "system",
					"ppp123");
			}catch(Exception e){
				
			}
		}
	}
	
	public void executeSQLCommand(String command){
		
	}
}
